{
  "root": "packages/analytics-integration",
  "sourceRoot": "packages/analytics-integration/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/analytics-integration/**/*.{ts,tsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/analytics-integration"
      ],
      "options": {
        "jestConfig": "packages/analytics-integration/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "scope:integration"
  ]
}
